{
  "allOf": [
    {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid",
          "readOnly": true
        },
        "email": {
          "type": "string",
          "format": "email",
          "maxLength": 254,
          "writeOnly": true
        },
        "password": {
          "type": "string",
          "format": "password",
          "writeOnly": true
        }
      }
    },
    {
      "$ref": "./tokens.json"
    }
  ]
}
